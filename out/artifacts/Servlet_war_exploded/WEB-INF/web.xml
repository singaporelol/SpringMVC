         version="4.0">
    
    <servlet>
        <servlet-name>helloServlet</servlet-name>
        <servlet-class>com.learn.javaweb.servlet.HelloServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>helloServlet</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>
    <listener>
        <listener-class>com.learn.javaweb.servlet.MyServletContextListener</listener-class>
    </listener>

